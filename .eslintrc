{
  "env": {
    "browser": true,
    "node": true,
    "jest": true,
    "es6": true
  },
  "parserOptions": {
    "ecmaVersion": 9,
    "sourceType": "module",
    "ecmaFeatures": {
      "jsx": true
    }
  },
  "extends": [
    "airbnb"
  ],
  "rules": {
    "prefer-destructuring": ["error", {
      "array": true,
      "object": true
    }],
    "import/prefer-default-export": "off",
    "func-names": ["error", "never"],
    "react/jsx-filename-extension": [0]
  }
}
