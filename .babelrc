{
	"presets": 
		[
			"@babel/preset-react",
      "@babel/preset-env",
      [
        "env",
        {
          "targets": {
            "browsers": "last 2 Firefox versions, last 2 Chrome versions, last 2 Edge versions, last 2 Safari versions"
          }
        }
      ]
  ],
  "plugins": [
    "@babel/plugin-proposal-object-rest-spread",
    "@babel/plugin-transform-runtime"
  ]
}
